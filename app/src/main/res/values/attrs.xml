<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BubbleView">
        <attr name="bubble_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="CirclePercentView">
        <attr name="back_circle_color" format="color"/>
        <attr name="percent_circle_color" format="color"/>
        <attr name="percent_circle_width" format="dimension"/>
        <attr name="percent_text_size" format="dimension"/>
        <attr name="percent_text_color" format="color"/>
        <attr name="percent_index" format="integer"/>
    </declare-styleable>
    <declare-styleable name="RecyclerViewPager">
        <attr name="rvp_triggerOffset" format="float" />
        <attr name="rvp_flingFactor" format="float" />
        <attr name="rvp_singlePageFling" format="boolean" />
    </declare-styleable>
    <declare-styleable name="MultiScrollNumber">
        <attr name="primary_number" format="integer"/>
        <attr name="target_number" format="integer"/>
        <attr name="number_size" format="integer"/>
        <!--<attr name="number_color" format="color"/>-->
    </declare-styleable>
</resources>